{
  "models": {
    "providers": {
      "ollama": {
        "baseUrl": "http://ollama:11434/v1",
        "apiKey": "ollama-local",
        "models": [
          {
            "id": "qwen3-coder:30b",
            "contextWindow": 65536,
            "maxOutputTokens": 8192
          }
        ]
      }
    }
  },
  "browser": {
    "enabled": true,
    "defaultProfile": "openclaw",
    "evaluateEnabled": true,
    "profiles": {
