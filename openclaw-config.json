        "cdpUrl": "http://browser:9223"
      }
    }
  },
  "tools": {
    "deny": ["web_search", "web_fetch"]
  },
  "channels": {
    "telegram": {
      "enabled": true,
      "dmPolicy": "allowlist",
      "allowFrom": ["917548904"],
      "streaming": true,
      "replyToMode": "first",
      "textChunkLimit": 4000
    }
  },
  "gateway": {
    "bind": "lan",
    "trustedProxies": ["172.16.0.0/12", "127.0.0.1"]
  }
